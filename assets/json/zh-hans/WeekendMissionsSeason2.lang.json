        "PortalAddress": "21DE0936B5C7",
        "NpcMessageFlows": {
          "IncomingMessages": [
            "阿里阿德涅回来了！我们的朋友很安全！啊，好欢喜！"
          ],
          "Options": [
            {
              "Name": "欣喜万分",
              "IfSelected": {
